    "@remix-run/react": "^2.15.3",
    "@remix-run/serve": "^2.15.3",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "isbot": "^4.1.0",
    "lucide-react": "^0.475.0",
    "node-cron": "^3.0.3",
    "prisma": "^6.4.1",
    "react": "^18.2.0",
